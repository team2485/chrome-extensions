{
  "name": "The W.A.R. Lords Theme Aero",
  "version": "1.0",
  "manifest_version": 2,
  "description": "The official Chrome theme of Team 2485, the W.A.R. (We Are Robot) Lords, a FIRST robotics team.",
  "icons": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
   },
  "theme": {
    "images": {
      "theme_toolbar": "images/theme_toolbar.png",
      "theme_ntp_background": "images/theme_stripe.png",
      "theme_ntp_attribution": "images/logo.png"
    },
    "colors": {
      "toolbar": [203, 161, 53],
      "tab_text": [17, 17, 17],
      "bookmark_text": [17, 17, 17],
      "ntp_background": [17, 17, 17],
      "ntp_text": [255, 255, 255],
      "ntp_link": [203, 161, 53],
      "ntp_header": [255, 255, 255],
      "ntp_section": [255, 255, 255],
      "ntp_section_text": [255, 255, 255],
      "ntp_section_link": [203, 161, 53],
      "button_background": [40, 40, 40]
    },
    "properties": {
      "ntp_background_repeat": "repeat",
      "ntp_logo_alternate": 1
    }
  }
}
